{
  "version": 2,
  "builds": [
    { "src": "api/[...all].js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/[...all].js" },
    { "src": "/admin", "dest": "/admin/index.html" },
    { "src": "/admin/(.*)", "dest": "/admin/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
